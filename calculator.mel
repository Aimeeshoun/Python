
//Addition

global float $result;

global proc float Add(float $numbers[])
{
float $sum = 0;

for ($i=0; $i<size($numbers); $i++)
{
$sum += $numbers[$i];
}

return $sum;
}

Add({2,3,4,5,6,4});
Add({1,2,4,45,6,5});

