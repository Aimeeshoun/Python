//Multiplication



global float $result;

global proc float Multiply(float $numbers[])
{
float $multiply = 1;

for ($i=1; $i<size($numbers); $i++)
{
$multiply *= $numbers[$i];
}
return $multiply;
}

Multiply({4,4});

//ivision

global float $result;

global proc float Divide(float $numbers[])
{
float $division = 0;

for ($i=1; $i<size($numbers); $i++)
{
$division /= $numbers[$i];
}
return $division;
}   
Divide({12,4});
